{
  "configurations": [
    {
      "type": "java",
      "name": "Ground",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "mainClass": "jk.kamoru.ground.GroundApplication",
      "vmArgs": "-Dspring.profiles.active=ground-home"
    },
    {
      "type": "java",
      "name": "Ground test",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "mainClass": "jk.kamoru.ground.GroundApplication",
      "vmArgs": "-Dspring.profiles.active=ground-test"
    },
    {
      "type": "java",
      "name": "Ground SSL",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "mainClass": "jk.kamoru.ground.GroundApplication",
      "vmArgs": "-Dspring.profiles.active=ground-home -Dserver.port=443 -Dserver.ssl.enabled=true -Dserver.ssl.key-alias=kamoru -Dserver.ssl.key-store=classpath:cert/kamoru.jk.p12 -Dserver.ssl.key-store-password=697489"
    },
    {
      "type": "java",
      "name": "Ground SSL test",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "mainClass": "jk.kamoru.ground.GroundApplication",
      "vmArgs": "-Dspring.profiles.active=ground-test -Dserver.port=443 -Dserver.ssl.enabled=true -Dserver.ssl.key-alias=kamoru -Dserver.ssl.key-store=classpath:cert/kamoru.jk.p12 -Dserver.ssl.key-store-password=697489"
    }
  ]
}